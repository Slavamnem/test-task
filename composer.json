{
    "name": "slavamnem/test",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "csa/guzzle-bundle": "^3.2",
        "doctrine/annotations": "^1.13",
        "jms/serializer-bundle": "^4.0",
        "symfony/console": "5.2.*",
        "symfony/validator": "^5.4",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {}
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {},
    "scripts": {},
    "conflict": {},
    "extra": {}
}
