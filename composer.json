{
    "name": "slavamnem/test",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "doctrine/annotations": "^1.13",
        "symfony/http-client": "5.2.*",
        "symfony/console": "5.4.*",
        "symfony/validator": "^5.4",
        "symfony/yaml": "5.4.*",
        "symfony/dotenv": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "v3.5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
